        				<div class="row">
        					<div class="container admin_settings_panel">
    				    		<div class="row" style="margin-top: 0px;">
                        			<div class="admin_settings_header" style="background-color: {{ main_color_scheme }}">
                        				<h4 style="color: {{ header_color_scheme }}">Titles</h4>
                        			</div>
                        		</div>
                        		<div class="row admin_settings_content">
                					<div class="row">{{ form_label(form.title) }} {{ form_widget(form.title) }}</div>
                					<div class="row">{{ form_label(form.short_title) }} {{ form_widget(form.short_title) }}</div>
                				</div>
                			</div>	
                		</div>
                		<div class="row">
							<div class="container admin_settings_panel">
    				    		<div class="row" style="margin-top: 0px;">
                        			<div class="admin_settings_header" style="background-color: {{ main_color_scheme }}">
                        				<h4 style="color: {{ header_color_scheme }}">Version</h4>
                        			</div>
                        		</div>
                        		<div class="row admin_settings_content">
                					<div class="row">{{ form_label(form.version) }} {{ form_widget(form.version) }}</div>
                				</div>
                			</div>
                		</div>		
                		<div class="row">
                			<div class="container admin_settings_panel">		
                				<div class="row" style="margin-top: 0px;">
                        			<div class="admin_settings_header" style="background-color: {{ main_color_scheme }}">
                        				<h4 style="color: {{ header_color_scheme }}">Url</h4>
                        			</div>
                        		</div>
                        		<div class="row admin_settings_content">
                					<div class="row">{{ form_label(form.url) }} {{ form_widget(form.url) }}</div>
                				</div>
                			</div>
						</div>
						<div class="row">
            					<div class="container admin_settings_panel">
            				        <div class="row" style="margin-top: 0px;">
                            			<div class="admin_settings_header" style="background-color: {{ main_color_scheme }}">
                            				<h4 style="color: {{ header_color_scheme }}">Color Scheme</h4>
                            			</div>
                            		</div>
                					<div class="row admin_settings_content">
                						<div class="col-lg-6">
                        					<div class="row">
                        					    {{ form_label(form.main_color_scheme) }} 
                        					</div>
                        					<div class="row">
                        	                    {{ form_widget(form.main_color_scheme) }}
                        	                </div>
                        	                <div class="row">
                        					    {{ form_label(form.header_color_scheme) }} 
                        					</div>
                        					<div class="row">
                        	                    {{ form_widget(form.header_color_scheme) }}
                        	                </div>
                    	            	</div>
                    	            	<div class="col-lg-6">
                        	                <div class="row">
                        					    {{ form_label(form.logo_color_scheme) }} 
                        					</div>
                        					<div class="row">
                        	                    {{ form_widget(form.logo_color_scheme) }}
                        	                </div>
                        	                <div class="row">
                        					    {{ form_label(form.button_color_scheme) }} 
                        					</div>
                        					<div class="row">
                        	                    {{ form_widget(form.button_color_scheme) }}
                        	                </div>
                	                	</div>
                	                </div>
                	            </div>
            	            </div>
        					<div class="row">
            					<div class="container admin_settings_panel">
        							<div class="row"  style="margin-top: 0px;">
                            			<div class="admin_settings_header" style="background-color: {{ main_color_scheme }}">
