            							<div class="row">{{ form_widget(form.footer) }}</div>
            						</div>
            					</div>
            				</div>

        				<div class="row">
        					<div style="width: 90%;">
        						<div style="float: right;"> 
        							<a href="{{ path('user_register') }}" class="btn btn-primary" style="background-color: {{ main_color_scheme }};">Register New Admin</a>
        						</div>
        					</div>
        				</div>
    				</div>
    			</div>
    		
