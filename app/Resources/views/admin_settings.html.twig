                                 			    </div>
    
                                 			    <div style="display: table-cell;padding-top: 10px; padding-right: 10px; width: 60px;">
                                 			        {{ form_widget(interaction_category.color_scheme) }}
                                 				</div>
                                 				<div style="display: table-cell;padding-top: 10px; padding-right: 10px; width: 300px;">
                                 			        {{ form_widget(interaction_category.description) }}
                                 				</div>
                                 			</div>
    
                             			{% endfor %}
