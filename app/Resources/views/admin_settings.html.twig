                                 			<div style="display: table-row; ">
                                 			    <div style="display: table-cell;padding-top: 10px; padding-right: 10px; width: 100px;">
                                 			        {{ form_widget(interaction_category.category_name) }}
                                 				</div>
                                 				<div style="display: table-cell;padding-top: 10px; padding-right: 10px; width: 30px;">
