    					<div class="row">{{ form_label(form.faq) }} {{ form_widget(form.faq) }}</div>
    				</div>
    			</div>
    			<div id="contact" class="tab-pane fade">
    				<div class="container">
    					<div class="row">{{ form_label(form.contact) }} {{ form_widget(form.contact) }}</div>
    				</div>
    			</div>
    			<div id="download" class="tab-pane fade">
    				<div class="container">
    				    <div class="row">{{ form_label(form.show_downloads) }} {{ form_widget(form.show_downloads) }}</div>
    					<div class="row">{{ form_label(form.show_download_all) }} {{ form_widget(form.show_download_all) }}</div>
    					<div class="row">{{ form_label(form.download) }} {{ form_widget(form.download) }}</div>

    				</div>
    			</div>
    		</div>
    		<div class="container">
    			<div class="row">
    			        {{ form_label(form.submit) }} {{ form_widget(form.submit) }}
    			</div>
    		</div>
    	</div>
    	{{ form_end(form) }} 
    </div>
</main>
<div class="shadow"></div>
<div class="shadow"></div>
<footer>
	
	{{ footer | raw }}
</footer>
{% endblock %} 
